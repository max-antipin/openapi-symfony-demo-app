{
    "name": "max-antipin/openapi-symfony-app-demo",
    "license": "MIT",
    "type": "project",
    "description": "OpenAPI-Symfony Demo Application",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/yaml": "7.1.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-symfony": "^1.4",
        "squizlabs/php_codesniffer": "^3.10",
        "phpunit/phpunit": "^11.3",
        "symfony/browser-kit": "7.1.*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.1.*"
        }
    }
}
